{
  "mcpServers": {
    "rag-cli": {
      "command": "python",
      "args": [
        "-m",
        "plugin.mcp.unified_server"
      ],
      "env": {
        "PYTHONUNBUFFERED": "1",
        "RAG_CLI_MODE": "claude_code",
        "PYTHONPATH": "/absolute/path/to/RAG-CLI/src",
        "RAG_CLI_ROOT": "/absolute/path/to/RAG-CLI"
      }
    }
  }
}
